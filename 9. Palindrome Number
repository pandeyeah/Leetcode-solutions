class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0)
            return false;
			
        string ans=to_string(x);
        int i=0;int j=ans.length()-1;
		
		
		
        while(i<j)
        {
            if(ans[i]==ans[j])
            {i++;j--;}
            else
                return false;
        }
	
        return true;
    }
};
